<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">  
<mapper namespace="mybatis.MyMemberImpl">
	<select id="login" resultType="mybatis.MemberVO">
		select * from member where id=#{0} and pass=#{1}
	</select>
	
	<insert id="insertjoin">
		insert into member(id,pass,name,email,mobile,birth) values (#{0},#{1},#{2},#{3},#{4},#{5})
   	</insert>
   	<insert id="email">
		insert into booking(idx,name,mobile,email,contents,flag) values (#{0},#{1},#{2},#{3},#{4},#{5})
   	</insert>
   	
</mapper>
